# Oshified Node Configuration
# Copy this file to .env and fill in your actual values

# Cloudflare R2 (S3-compatible) Configuration
S3_ENDPOINT_URL=https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
S3_ACCESS_KEY_ID=your_r2_access_key_here
S3_SECRET_ACCESS_KEY=your_r2_secret_key_here
S3_BUCKET_NAME=oshified-exports
S3_REGION_NAME=auto

# Oshified Backend URL
OSHIFIED_BACKEND_URL=https://oshified-backend.your-subdomain.workers.dev

# Optional: API endpoint for logging (can leave empty)
OSHIFIED_API_ENDPOINT=

# YouTube MP3 API Keys (RapidAPI) - For fallback when yt-dlp is blocked
# Get from: https://rapidapi.com/ytjar/api/youtube-mp36
# Basic plan: 300 requests/month free
RAPIDAPI_KEY_1=your_rapidapi_key_here
RAPIDAPI_KEY_2=
RAPIDAPI_KEY_3=
RAPIDAPI_KEY_4=
